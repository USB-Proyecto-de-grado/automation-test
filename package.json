{
  "name": "automation-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:ui": "mocha  --experimental-modules 'tests/ui/**/*.spec.js'  --reporter mochawesome --reporter-options reportDir=reports/ui,reportFilename=uiReport,charts=true",
    "test:api": "mocha 'tests/api/**/*.js' --reporter mochawesome --reporter-options reportDir=reports/api,reportFilename=apiReport,charts=true",
    "test:custom": "mocha 'tests/**/*.js' --grep \"\\[Tag: YourCustomTag\\]\"  --reporter mochawesome --reporter-options reportDir=reports/custom,reportFilename=customReport,overwrite=false,charts=true",
    "test:ids": "mocha 'tests/**/*.js' --grep $npm_config_ids --reporter mochawesome --reporter-options reportDir=reports/custom,reportFilename=customReport,overwrite=false,charts=true",
    "test": "npm run test:ui && npm run test:api",
    "start-gui": "electron main/app/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.5.0",
    "electron": "^33.0.2",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "chokidar": "^4.0.1",
    "chromedriver": "^128.0.1",
    "geckodriver": "^4.5.1",
    "selenium-webdriver": "^4.24.0"
  }
}
